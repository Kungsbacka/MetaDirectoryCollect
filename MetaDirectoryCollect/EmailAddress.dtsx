                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">DECLARE @year int
SELECT @year = CAST(CONVERT(char(2), GETDATE(), 12) AS int)
SELECT
   N'Medvind' AS sourcename,
   REALPNR AS sourcepid,
   CASE
      WHEN CAST(SUBSTRING(REALPNR, 1, 2) AS int) &gt; @year
      THEN N'19' + SUBSTRING(REALPNR, 1, 6) + SUBSTRING(REALPNR, 8, 4)
      ELSE N'20' + SUBSTRING(REALPNR, 1, 6) + SUBSTRING(REALPNR, 8, 4)
   END AS pid,
   NULLIF(EMAIL, '') AS mail
FROM
   dbo.Person</property>
